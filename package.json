{
  "name": "kingyohime",
  "version": "0.0.1",
  "description": "A manga browser",
  "scripts": {
    "dev": "webpack --watch --config webpack.config.dev.ts",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "build": "npm run clean; webpack --config webpack.config.prod.ts",
    "start": "electron ./dist/main.js",
    "clean": "rm -rf ./dist",
    "package": "npm run build && electron-builder -c.extraMetadata.main=./dist/main.js  --linux deb",
    "test": "npm run build && jest"
  },
  "build": {
    "productName": "Kingyohime",
    "appId": "kingyohime.nryotaro.dev",
    "files": [
      "dist/**"
    ]
  },
  "repository": "https://github.com/nryotaro/kingyohime",
  "keywords": [
    "Electron",
    "typescript"
  ],
  "author": {
    "name": "Nakamura, Ryotaro",
    "email": "nakamura.ryotaro@ryotaro.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.23",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "copy-webpack-plugin": "^6.2.1",
    "electron": "^9.3.1",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.1.0",
    "eslint": "^7.3.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "spectron": "^12.0.0",
    "ts-loader": "^8.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "samba-client": "^3.2.0",
    "styled-components": "^5.2.0"
  }
}
